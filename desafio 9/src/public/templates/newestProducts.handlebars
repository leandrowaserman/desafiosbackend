<table class="table-products">
    <thead key="thead">
    <tr key="title">
        <th>Nombre</th>
        <th>Precio</th>
        <th>Imagen</th>
    </tr>
    </thead>
    <tbody key="tbody">
        {{#each products}}
        <tr>
            <td>{{this.name}}</td>
            <td>{{this.price}}</td>
            <td><img src={{this.thumbnail}} width="40px"></td>
        </tr>
        {{/each}}
    </tbody>
</table>